<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add to VCS">
        <Item>
          <IdeaActionID>ChangesView.AddUnversioned</IdeaActionID>
          <ShortCut>'Ctrl+Alt+A'</ShortCut>
          <Description>Add to VCS</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Left'</ShortCut>
          <Description>Back</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'F12' or 'Shift+F2' or 'Ctrl+B'</ShortCut>
          <Description>Declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'Delete'</ShortCut>
          <Description>Delete</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'Ctrl+V' or 'Shift+Insert'</ShortCut>
          <Description>Paste</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'Alt+4'</ShortCut>
          <Description>Run</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Update Snapshot Tests'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Run 'Update Snapshot Tests'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Wallaby'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Run 'Wallaby'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'scratch.js'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'Ctrl+F9'</ShortCut>
          <Description>Run 'scratch.js'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'sparkline'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'Ctrl+F9'</ShortCut>
          <Description>Run 'sparkline'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Scratch File">
        <Item>
          <IdeaActionID>NewScratchFile</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+Insert'</ShortCut>
          <Description>Scratch File</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'start'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Shift+F5'</ShortCut>
          <Description>Stop 'start'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>TODO</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Shift+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Rerun 'rollup'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <Description>Rerun 'rollup'</Description>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'sparkline'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <Description>Rerun 'sparkline'</Description>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Run Regression Tests'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Run 'Run Regression Tests'</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>